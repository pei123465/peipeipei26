Sheet2!B2

=LET(
  p, TEXTSPLIT(A2,"."),
  INDEX(p,1)*256^3 +
  INDEX(p,2)*256^2 +
  INDEX(p,3)*256 +
  INDEX(p,4)
)


Sheet1!D2（開始IP）

=LET(
  p, TEXTSPLIT(TEXTBEFORE(A2,"/"),"."),
  INDEX(p,1)*256^3 +
  INDEX(p,2)*256^2 +
  INDEX(p,3)*256 +
  INDEX(p,4)
)

Sheet1!E2（終了IP）

=LET(
  start, D2,
  size, 2^(32-VALUE(TEXTAFTER(A2,"/"))),
  start + size - 1
)


Sheet2!C2
=IF(
  COUNTIFS(
    Sheet1!B:B,"済",
    Sheet1!C:C,"有効",
    Sheet1!D:D,"<="&B2,
    Sheet1!E:E,">="&B2
  )>0,
  "済",
  "未"
)

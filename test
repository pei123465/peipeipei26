=LET(
  src, A:A,
  dom, FILTER(TRIM(src), TRIM(src)<>""),          /* 空白除去 + 前後スペース除去 */
  u, UNIQUE(dom),                                 /* 重複削除 */
  w, FILTER(u, LEFT(u,1)="."),                    /* ワイルドカードだけ */
  n, FILTER(u, LEFT(u,1)<>".", ""),               /* 非ワイルドカード */
  w_keep,
    FILTER(
      w,
      BYROW(
        w,
        LAMBDA(x,
          SUM(--((LEN(w)<LEN(x))*(RIGHT(x,LEN(w))=w)))=0
        )
      )
    ),
  SORT(VSTACK(n, w_keep))
)
